    <section id="latest-blog" class="latest-blog section">
        <div class="container">
            <h2 class="title text-center">Des idées de voyages :</h2>
            <div class="row">
                <div class="item col-md-6 col-12">
                    <div class="item-inner">
                        <figure class="figure images-box">
                            <a href="blog-post.html"><%= image_tag("thailand-2.jpg") %></a>
                        </figure>
                        <div class="content-wrapper text-center">
                            <h3 class="sub-title"><a href="blog-post.html">Les choses à faire en Thaïlande</a></h3>
